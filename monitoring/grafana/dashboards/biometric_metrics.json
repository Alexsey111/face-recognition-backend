          { "text": "1h", "value": "1h" },
          { "text": "6h", "value": "6h" },
          { "text": "12h", "value": "12h" },
          { "text": "1d", "value": "1d" }
        ],
        "query": "1m,5m,15m,30m,1h,6h,12h,1d",
        "skipUrlSync": false,
        "type": "interval"
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {
    "nowDelay": "",
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "timezone": "UTC",
  "title": "Biometric Verification Metrics",
  "uid": "biometric-metrics",
  "version": 1,
  "weekStart": ""
}