{
  "annotations": {
    "list": [
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "enable": true,
        "expr": "ALERTS{alertname=~\"HighFAR|HighFRR|HighEER\"}",
        "iconColor": "red",
        "name": "Biometric Alerts",
        "titleFormat": "Alert: {{alertname}}"
      }
    ]
  },
  "description": "Biometric verification FAR/FRR metrics with compliance monitoring for 152-FZ",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "FAR/FRR Compliance (TZ Requirements)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "targets": [
        { "expr": "false_accept_rate", "legendFormat": "FAR", "refId": "A" },
        { "expr": "false_reject_rate", "legendFormat": "FRR", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "colorMode": "background",
          "mappings": [],
          "noValue": "No data",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "orange", "value": 0.5 },
              { "color": "red", "value": 1.0 }
            ]
          },
          "unit": "percent"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "FAR" },
            "properties": [
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "red", "value": 0.1 }
                  ]
                }
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "FRR" },
            "properties": [
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "red", "value": 3.0 }
                  ]
                }
              }
            ]
          }
        ]
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Equal Error Rate (EER)",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "targets": [{ "expr": "equal_error_rate", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 10,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1.0 },
              { "color": "orange", "value": 2.0 },
              { "color": "red", "value": 3.0 }
            ]
          },
          "unit": "percent"
        }
      },
      "options": {
        "min": 0,
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 3,
      "title": "Compliance Status",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "targets": [{ "expr": "1", "legendFormat": "Overall Compliant", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            {
              "options": {
                "0": { "color": "red", "text": "NON-COMPLIANT" },
                "1": { "color": "green", "text": "COMPLIANT" }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "FAR/FRR Trend (24h)",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 8 },
      "targets": [
        { "expr": "false_accept_rate", "legendFormat": "FAR", "refId": "A" },
        { "expr": "false_reject_rate", "legendFormat": "FRR", "refId": "B" },
        { "expr": "equal_error_rate", "legendFormat": "EER", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "line" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.1 }
            ]
          },
          "unit": "percent"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "FAR" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } },
              { "id": "custom.lineWidth", "value": 2 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "FRR" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } },
              { "id": "custom.lineWidth", "value": 2 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "EER" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } },
              { "id": "custom.lineWidth", "value": 2 }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "lastNotNull"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": { "mode": "multi", "sort": "none" }
      }
    },
    {
      "id": 5,
      "title": "False Accepts by Severity",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 17 },
      "targets": [
        { "expr": "rate(false_accept_total{severity=\"high\"}[5m])", "legendFormat": "High Severity", "refId": "A" },
        { "expr": "rate(false_accept_total{severity=\"medium\"}[5m])", "legendFormat": "Medium Severity", "refId": "B" },
        { "expr": "rate(false_accept_total{severity=\"low\"}[5m])", "legendFormat": "Low Severity", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "events/sec",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "normal" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [] },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "calcs": ["sum"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": { "mode": "multi", "sort": "none" }
      }
    },
    {
      "id": 6,
      "title": "False Rejects by Severity",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 17 },
      "targets": [
        { "expr": "rate(false_reject_total{severity=\"high\"}[5m])", "legendFormat": "High Severity", "refId": "A" },
        { "expr": "rate(false_reject_total{severity=\"medium\"}[5m])", "legendFormat": "Medium Severity", "refId": "B" },
        { "expr": "rate(false_reject_total{severity=\"low\"}[5m])", "legendFormat": "Low Severity", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "events/sec",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "normal" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [] },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "calcs": ["sum"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": { "mode": "multi", "sort": "none" }
      }
    },
    {
      "id": 7,
      "title": "Liveness Detection Rate",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 26 },
      "targets": [
        {
          "expr": "sum(rate(liveness_checks_total{result=\"live\"}[5m])) / sum(rate(liveness_checks_total[5m])) * 100",
          "legendFormat": "Live Detection Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "colorMode": "background",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 98 }
            ]
          },
          "unit": "percent"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "gauge",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 8,
      "title": "Spoofing Attacks Detected",
      "type": "piechart",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 26 },
      "targets": [{ "expr": "sum by (attack_type) (spoofing_attacks_detected)", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [] }
        }
      },
      "options": {
        "displayLabels": ["name", "percent"],
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "values": [true, true]
        },
        "pieType": "donut",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "id": 9,
      "title": "Verification Response Time (P95)",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 26 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(verification_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "P95 Response Time",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 1000,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "orange", "value": 800 },
              { "color": "red", "value": 1000 }
            ]
          },
          "unit": "ms"
        }
      },
      "options": {
        "min": 0,
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 10,
      "title": "Verification Throughput",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 26 },
      "targets": [{ "expr": "rate(verification_requests_total[5m])", "legendFormat": "Verifications/sec", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "verifications/sec",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [] },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": { "mode": "multi", "sort": "none" }
      }
    },
    {
      "id": 11,
      "title": "Verification Confidence Distribution",
      "type": "heatmap",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 32 },
      "targets": [
        {
          "expr": "sum(rate(verification_confidence_score_bucket[5m])) by (le, result)",
          "format": "heatmap",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [] }
        }
      },
      "options": {
        "calculate": true,
        "cellGap": 2,
        "cellRadius": 2,
        "cellValues": { "decimals": 1, "unit": "short" },
        "color": { "scheme": "Spectral", "mode": "scheme" },
        "exemplars": { "color": "rgba(255,255,255,0.5)" },
        "yAxis": { "decimals": 1, "unit": "short" }
      }
    },
    {
      "id": 12,
      "title": "GPU Utilization",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 32 },
      "targets": [{ "expr": "gpu_utilization_percent", "legendFormat": "GPU {{gpu_id}}", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "GPU %",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "line+area" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 90 }
            ]
          },
          "unit": "percent"
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": { "mode": "multi", "sort": "none" }
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["biometric", "security", "compliance", "152-FZ"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      },
      {
        "current": { "selected": true, "text": "5m", "value": "5m" },
        "hide": 0,
        "includeAll": false,
        "label": "Time Range",
        "multi": false,
        "name": "interval",
        "options": [
          { "text": "1m", "value": "1m" },
          { "text": "5m", "value": "5m" },
          { "text": "15m", "value": "15m" },
